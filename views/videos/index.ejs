<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <title>Video Library</title>
</head>
<body>
    <h1> Video Library </h1>
    <ul id="list">
        <% videos.forEach((video) => { %>
            <li>
                <button id="videoList">
                    <a href="/videos/<%=video._id%>">
                    <%=video.title%>
                    </a>
                </button>
            </li>
        <% }) %>
    </ul>
    <button class="buttonText">
        <a href="/videos/new">Add new video!</a>
    </button>
    <button class="buttonText">
        <a href="/">Back to Home</a>
    </button>
</body>
</html>